{
  "extends": [
    "development"
  ],
  "hints": {
    "apple-touch-icons": "off",
    "disown-opener": "off",
    "no-inline-styles": "off",
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off",
        "button-name": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "frame-title": "off",
        "image-alt": "off"
      }
    ],
    "compat-api/css": "off",
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "meta-viewport": "off",
    "button-type": "off",
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 125",
    "not chrome <= 126",
    "not edge <= 125",
    "not ios_saf <= 17.5",
    "not opera <= 110",
    "not safari <= 17.5",
    "not samsung <= 25"
  ]
}